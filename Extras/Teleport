local Main = loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/JixSo-48/CodeNest/refs/heads/main/Extras/Main", true))()
spawn(function()
    while wait() do
        pcall(function()
            Main.ServerHop()
        end)
    end
end)
if not isfile("num_teleports.txt") then
    writefile("num_teleports.txt", "0")
end

writefile("num_teleports.txt", tostring(tonumber(readfile("num_teleports.txt"))+1))
